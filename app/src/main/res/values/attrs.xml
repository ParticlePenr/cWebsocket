        <attr name="fab_cradle_rounded_corner_radius" format="dimension" />
        <attr name="cradle_vertical_offset" format="dimension" />
        <attr name="fab_size" format="dimension" />
        <attr name="mCardViewStyle" format="reference" />
    </declare-styleable>

    <declare-styleable name="FullscreenAttrs">
        <attr name="fullscreenBackgroundColor" format="color" />
        <attr name="fullscreenTextColor" format="color" />
