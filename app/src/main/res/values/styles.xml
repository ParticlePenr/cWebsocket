<?xml version="1.0" encoding="utf-8"?>
<resources xmlns:tools="http://schemas.android.com/tools">

    <style name="CardViewStyle" parent="CardView">
        <item name="cardCornerRadius">10dp</item>
        <item name="cardElevation">6dp</item>
        <item name="cardBackgroundColor" tools:targetApi="l">?attr/colorPrimary</item>
    </style>

    <style name="ErrorTextAppearance" parent="TextAppearance.AppCompat">
        <item name="android:textColor">?attr/colorError</item>
        <item name="android:textSize">16sp</item>
        <item name="android:textStyle">bold|italic</item>
    </style>

    <style name="EditTextThemeOverlay" parent="ThemeOverlay.AppCompat">
        <item name="colorAccent">?attr/colorSecondaryVariant</item>
    </style>

    <style name="mButton" parent="TextAppearance.AppCompat.Button">
        <item name="fontFamily">@font/impact_font_family</item>
    </style>


    <style name="mTextInputLayout" parent="Widget.MaterialComponents.TextInputLayout.OutlinedBox">
        <item name="android:colorControlActivated">?attr/colorSecondary</item>
        <item name="boxBackgroundColor">?attr/colorPrimaryDark</item>
        <item name="hintTextColor">?attr/colorOnPrimary</item>
        <item name="android:textColorHint">?attr/colorOnPrimary</item>
        <item name="android:textColor">?attr/colorOnPrimary</item>
    </style>

    <style name="mEditText" parent="Widget.AppCompat.EditText">
        <item name="android:background">?attr/colorPrimarySurface</item>
        <item name="android:fontFamily">@font/muli_font_family</item>
    </style>

    <style name="firtsRowTable">


    </style>
</resources>